{
  "name": "bun-plugin-dts",
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "description": "A bun plugin for generating dts files",
  "scripts": {
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build",
    "release": "bumpp && npm publish",
    "lint": "biome check src",
    "format": "biome format src --write"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bun"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/bun-plugin-dts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/bun-plugin-dts.git"
  },
  "bugs": "https://github.com/wobsoriano/bun-plugin-dts/issues",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "dependencies": {
    "common-path-prefix": "^3.0.0",
    "dts-bundle-generator": "^9.5.1",
    "get-tsconfig": "^4.13.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "bumpp": "^10.4.1",
    "@types/bun": "^1.3.9",
    "typescript": "^5.4.5"
  }
}
